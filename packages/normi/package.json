{
  "name": "@rspc/normi",
  "version": "0.1.2",
  "description": "A normalised caching layer for rspc.",
  "keywords": [],
  "author": "Oscar Beaumont",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "tsup"
  },
  "dependencies": {
    "@rspc/client": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@rspc/react": "workspace:*",
    "@rspc/solid": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@rspc/react": {
      "optional": true
    },
    "@rspc/solid": {
      "optional": true
    }
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "splitting": true,
    "clean": true,
    "sourcemap": true
  }
}
